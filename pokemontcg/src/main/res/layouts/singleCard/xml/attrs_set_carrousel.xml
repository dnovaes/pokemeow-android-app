<?xml version="1.0" encoding="utf-8"?>
<LinearLayout>
    <androidx.constraintlayout.helper.widget.Carousel
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/bsheet_expset_carousel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:carousel_forwardTransition="@id/forward"
        app:carousel_backwardTransition="@id/backward"
        app:carousel_previousState="@id/previous"
        app:carousel_nextState="@id/next"
        app:carousel_infinite="false"
        app:carousel_firstView="@id/imageViewCenter"
        app:constraint_referenced_ids="imageViewOuterLeft,imageViewCloseLeft,imageViewCenter,imageViewCloseRight,imageViewOuterRight"
        tools:ignore="MissingDefaultResource" />
</LinearLayout>
